<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TerraMarcadaV2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TerraMarcadaV2.Views"
    xmlns:local2="clr-namespace:TerraMarcadaV2"
    Title="TerraMarcadaV2">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:Home}"
        Route="MainPage" />

    <ShellContent
     Title="Mapa"
     ContentTemplate="{DataTemplate local2:MainPage}"
     Route="Mapa" />

    <ShellContent
     Title="Coletora GNSS"
     ContentTemplate="{DataTemplate local:GnssPage}"
     Route="ColetoraGnss" />

    <ShellContent
     Title="GeoCamera"
     ContentTemplate="{DataTemplate local:GeoCamera}"
     Route="GeoCamera" />


</Shell>
